<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.uwaterloo.cs446.cs446project">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="FerryMan"
        android:roundIcon="@drawable/icon2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:enabled="true" android:name=".MusicService" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" />
        <activity android:name=".SelectionActivity" />
        <activity android:name=".OptionActivity" />
        <activity android:name=".ScoreActivity"></activity>
    </application>

</manifest>